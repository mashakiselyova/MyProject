@model IEnumerable<MyProject.Models.Vocabulary>

<div class="card">
    <ul class="list-group list-group-flush">
        @foreach (var vocabulary in Model)
        {
            <li class="list-group-item"><a asp-action="ShowVocabulary" asp-route-id="@vocabulary.Id">@vocabulary.Name</a></li>
        }
    </ul>
</div>

