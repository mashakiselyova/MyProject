@model IEnumerable<MyProject.Models.Collection>


<div class="card">
    <ul class="list-group list-group-flush">
        @foreach (var collection in Model)
        {
            <li class="list-group-item"><a asp-action="ShowCollection" asp-route-id="@collection.Id">@collection.Name</a></li>
        }
    </ul>
</div>

