@model MyProject.Models.Dictionary

    <form asp-controller="Dictionary" asp-action="AddDictionary" method="post">
        <div class="form-group">
            <label asp-for="LanguageFrom">Изучаемый язык</label>
            <input asp-for="LanguageFrom" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="LanguageTo">Язык перевода</label>
            <input asp-for="LanguageTo" class="form-control" />
        </div>
        <button type="submit" class="btn btn-primary">Добавить</button>
    </form>
